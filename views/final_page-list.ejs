    <script>
        function fetchEvents() {
            var token = localStorage.getItem("token");
            var myHeaders = new Headers();
            myHeaders.append("authorization", "Bearer " + token);

            fetch("/api/event", {
                method: "GET",
                mode: "cors",
                cache: "no-cache",
                credentials: "same-origin",
                headers: myHeaders,
                redirect: "follow",
                referrerPolicy: "no-referrer",
            })
                .then((response) => response.json())
                .then((events) => {
                    let eventsContainer = document.getElementById(
                        "--js--events-tb-ulist"
                    );
                    // eventsContainer.innerHTML = '';
                    console.log(events);
                    events.forEach((event) => {
                        // --js--events-tb-ulist
                        let eventDiv = document.createElement("div");
                        eventDiv.className = "nk-tb-item";
                        eventDiv.innerHTML = `
                                                    <div class="nk-tb-col">
                                                        <a href="html/cms/page-sample.html" class="d-flex align-items-center gap-3">
                                                            <div class="user-avatar sq">
                                                                <img src="images/avatar/a-sm.jpg" alt="">
                                                            </div>
                                                            <span class="fw-medium">${event.title}</span>
                                                        </a>
                                                    </div>
                                                    <div class="nk-tb-col tb-col-mb">
                                                        <a href="#">
                                                            <div class="user-card">
                                                                <div class="user-avatar  sm bg-orange-dim">
                                                                        <span>A</span>
                                                                    </div>
                                                                    <div class="user-info">
                                                                        <span class="tb-lead">abubin_ishtiyak</span>
                                                                    </div>
                                                                </div>
                                                            </a>
                                                        </div>
                                                        <div class="nk-tb-col tb-col-sm">
                                                            <span>${event.type}</span>
                                                        </div>
                                                        <div class="nk-tb-col tb-col-md">
                                                            <div>
                                                                <span>Published</span>
                                                            </div>
                                                            <div>
                                                                <span>${new Date(event.date).toLocaleString()}</span>
                                                            </div>
                                                        </div>
                                                        <div class="nk-tb-col nk-tb-col-tools">
                                                            <ul class="nk-tb-actions gx-1">
                                                                <li class="nk-tb-action-hidden">
                                                                    <a class="btn btn-trigger btn-icon" data-bs-toggle="modal" href="#editPage" data-bs-placement="top" title="Quick Edit">
                                                                        <em class="icon ni ni-edit-fill"></em>
                                                                    </a>
                                                                </li>
                                                                <li class="nk-tb-action-hidden">
                                                                    <a href="html/cms/page-sample.html" class="btn btn-trigger btn-icon" data-bs-toggle="tooltip" data-bs-placement="top" title="View">
                                                                        <em class="icon ni ni-eye-fill"></em>
                                                                    </a>
                                                                </li>
                                                                <li class="nk-tb-action-hidden">
                                                                    <a href="#" class="btn btn-trigger btn-icon" data-bs-toggle="tooltip" data-bs-placement="top" title="Move To Trash">
                                                                        <em class="icon ni ni-trash-fill"></em>
                                                                    </a>
                                                                </li>
                                                                <li>
                                                                    <div class="drodown">
                                                                        <a href="#" class="dropdown-toggle btn btn-icon btn-trigger" data-bs-toggle="dropdown"><em class="icon ni ni-more-h"></em></a>
                                                                        <div class="dropdown-menu dropdown-menu-end">
                                                                            <ul class="link-list-opt no-bdr">
                                                                                <li><a href="#"><em class="icon ni ni-eye-fill"></em><span>View Page</span></a></li>
                                                                                <li><a data-bs-toggle="modal" href="#editPage"><em class="icon ni ni-edit-fill"></em><span>Edit Page</span></a></li>
                                                                                <li><a href="#"><em class="icon ni ni-trash-fill"></em><span>Trash</span></a></li>
                                                                            </ul>
                                                                        </div>
                                                                    </div>
                                                                </li>
                                                            </ul>
                                                        </div>
                                            
                    `;
                        eventsContainer.appendChild(eventDiv);
                    });
                })
                .catch((error) => {
                    console.error("Error fetching events:", error);
                });
        }

        fetchEvents();
    </script>
